<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat' 
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'
 	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
  xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client">
 
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="outerMargins">
    <ui:attributes top="20" right="20" bottom="20" left="20" />
  </ui:with>
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="northMargins">
    <ui:attributes top="5" right="5" bottom="5" left="5" />
  </ui:with>
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="westMargins">
    <ui:attributes top="0" right="5" bottom="0" left="5" />
  </ui:with>
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="centerMargins">
    <ui:attributes top="0" right="0" bottom="0" left="0" />
  </ui:with>
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="eastMargins">
    <ui:attributes top="0" right="5" bottom="0" left="5" />
  </ui:with>
  <ui:with type="com.sencha.gxt.core.client.util.Margins" field="southMargins">
    <ui:attributes top="5" right="5" bottom="5" left="5" />
  </ui:with>
 
  <ui:with type="com.sencha.gxt.widget.core.client.container.MarginData" field="outerData">
    <ui:attributes margins="{outerMargins}" />
  </ui:with>
  <ui:with type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData" field="northData">
    <ui:attributes size="100" margins="{northMargins}" />
  </ui:with>
  <ui:with type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData" field="westData">
    <ui:attributes size="150" margins="{westMargins}" collapsible="true" split="true" />
  </ui:with>
  <ui:with type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData" field="centerData">
    <ui:attributes size=".4" margins="{centerMargins}" />
  </ui:with>
  
  <!-- ui:with type="com.sencha.gxt.widget.core.client.container.MarginData" field="centerData">
    <ui:attributes margins="{centerMargins}" />
  </ui:with-->
  
  <ui:with type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData" field="eastData">
    <ui:attributes size=".5" margins="{eastMargins}" />
  </ui:with>
  <ui:with type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData" field="southData">
    <ui:attributes size="100" margins="{southMargins}" />
  </ui:with>
 
 
  <container:SimpleContainer>
    <container:child layoutData="{outerData}">
      <container:BorderLayoutContainer ui:field="con" borders="false">
        
        <!-- container:north layoutData="{northData}">
        </container:north -->
        
        <container:west layoutData="{westData}">
        	<!-- gxt:ContentPanel hideCollapseTool="false" bodyBorder="false" headerVisible="false" -->
        	<g:HTMLPanel>
        	<g:Button ui:field="btnAdd">Add Document</g:Button>
          	<div>
          		My Documents
				<ul>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=drafts" text="Drafts"></g:Hyperlink> 
					</li>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=inprog" text="In Progress"></g:Hyperlink> 
					</li>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=approved" text="Approved"></g:Hyperlink> 
					</li>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=rejected" text="Rejected"></g:Hyperlink> 
					</li>
				</ul>
			</div>
			<div>
				Approval Requests
				<ul>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=appreqnew" text="New Requests"></g:Hyperlink> 
					</li>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=appredone" text="Recent Approvals"></g:Hyperlink> 
					</li>
				</ul>
			</div>
			<div>
				Flagged
				<ol>
					<li> 
						<g:Hyperlink targetHistoryToken="home;type=flagged" text="Flagged"></g:Hyperlink> 
					</li>
				</ol>
			</div>
			<div>
				<ol>
					<li> 
						<g:Anchor ui:field="aLogout">Logout</g:Anchor> 
					</li>
				</ol>
			</div>			
          </g:HTMLPanel>
        </container:west>
        
        <container:center layoutData="{centerData}">
        	<g:HTMLPanel>
	          	<g:HTMLPanel>
					<g:Button ui:field="btnEdit">Edit Document</g:Button>
					<button>search</button>
					<button>filter</button>
					<g:Button ui:field="btnSimulate">Simulate</g:Button>
				</g:HTMLPanel>
			
				<g:HTMLPanel ui:field="container">
					
				</g:HTMLPanel>		
			</g:HTMLPanel>
        </container:center>
        
        
        <container:east layoutData="{eastData}">     
        	<g:HTMLPanel ui:field="docContainer">
       			
			</g:HTMLPanel>
        </container:east>
        
        <!-- container:south layoutData="{southData}">
          <gxt:ContentPanel />
        </container:south -->
      </container:BorderLayoutContainer>
    </container:child>
  </container:SimpleContainer>
</ui:UiBinder>